; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Untouched"
config/version="0.1"
run/main_scene="res://Scenes/IntroAnimation.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Untouched"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 0.4)
boot_splash/show_image=false
boot_splash/image="res://logo.png"
boot_splash/use_filter=false
config/icon="res://logo.png"

[autoload]

WorldGlobalVariables="*res://Scripts/WorldGlobalVariables.gd"
AugmentHolder="*res://Scripts/AugmentHolder.gd"
SaveManage="*res://Scripts/SaveManager.gd"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[debug]

file_logging/enable_file_logging=true

[dialogic]

directories/dch_directory={
"Master": "res://Dialogic/Characters/Master.dch"
}
directories/dtl_directory={
"timeline": "res://Dialogic/Timelines/testTimeline.dtl"
}
variables={}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autocolor_names=false
text/autopauses={}
layout/style_list=["res://Dialogic/Styles/main_style_novel.tres"]
layout/default_style="res://Dialogic/Styles/main_style_novel.tres"
glossary/default_case_sensitive=true

[display]

window/size/viewport_width=592
window/size/viewport_height=272
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/dialogic/plugin.cfg", "res://addons/label_font_auto_sizer/plugin.cfg", "res://addons/scene_manager/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "green",
"res://Dialogic/": "teal",
"res://Scenes/": "red",
"res://Scripts/": "blue"
}

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://Assets/main_theme.tres"
theme/custom_font="res://Assets/Fonts/mago1.ttf"

